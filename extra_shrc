#makes some shortcuts available for vim instead for shell
stty -ixon

#nvbn/thefuck
eval "$(thefuck --alias)"

# http://www.simononsoftware.com/virtualenv-tutorial-part-2/
export WORKON_HOME=$HOME/.virtualenvs
. /usr/local/bin/virtualenvwrapper.sh

if [ -e /opt/ros/kinetic/ ]; then
    . /opt/ros/kinetic/setup.$(basename $SHELL)
    echo Sourcing /opt/ros/kinetic/setup.$(basename $SHELL)
fi
